{
  "name": "FSharp-SyntaxTest",
  "version": "0.0.0",
  "publisher": "Ionide",
  "description": "Dev Tool for Quick Syntax Grammar Testing",
  "categories": [
    "Languages"
  ],
  "icon": "images/logo.png",
  "homepage": "http://ionide.io",
  "contributors": [
    {
      "name": "Jared Hester",
      "email": "jared.hester@gmail.com",
      "url": "https://twitter.com/cloudRoutine"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ionide/ionide-fsgrammar.git"
  },
  "engines": {
    "vscode": "^0.10.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "fsharp",
        "aliases": [
          "F# Syntax"
        ],
        "extensions": [
          ".fs",
          ".fsx",
          ".fsi"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "fsharp.output",
        "aliases": [
          "F# output channel"
        ],
        "mimetypes": [
          "text/x-code-output"
        ]
      }
    ],
    "grammars": [
      {
        "language": "fsharp.output",
        "scopeName": "output.fsharp",
        "path": "./syntaxes/fsharp.output.json"
      },
      {
        "language": "fsharp",
        "scopeName": "source.fsharp",
        "path": "./syntaxes/fsharp.json"
      },
      {
        "language": "fsharp",
        "scopeName": "source.fsharp.fsx",
        "path": "./syntaxes/fsharp.fsx.json"
      },
      {
        "language": "fsharp",
        "scopeName": "source.fsharp.fsi",
        "path": "./syntaxes/fsharp.fsi.json"
      },
      {
        "language": "fsharp",
        "scopeName": "source.fsharp.fsl",
        "path": "./syntaxes/fsharp.fsl.json"
      },
      {
        "language": "paket-dependencies",
        "scopeName": "source.paket.dependencies",
        "path": "./syntaxes/paket.dependencies.json"
      },
      {
        "language": "paket-lock",
        "scopeName": "source.paket.lock",
        "path": "./syntaxes/paket.lock.json"
      }
    ],
    "outputChannels": [
      "Textmate Tokens"
    ]
  },
  "extensionDependencies": [
    "vscode.fsharp"
  ],
  "scripts": {
    "build": "fable ../src/FSharp.SyntaxTools.fsproj -s -o ../fsharp.syntaxtest -m commonjs --verbose"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "fable-core": "^0.5.4",
    "node-gyp": "^3.4.0",
    "vscode-textmate": "^2.1.1"
  },
  "devDependencies": {
    "vscode": "^0.11.17",
    "fable-compiler": "^0.5.6"
  }
}
